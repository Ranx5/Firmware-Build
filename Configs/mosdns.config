config mosdns 'config'
        option enabled '1'
        option listen_port '5335'
        option geo_auto_update '1'
        option geo_update_week_time '*'
        option geo_update_day_time '5'
        option configfile '/var/etc/mosdns.json'
        option log_level 'error'
        option log_file '/var/log/mosdns.log'
        option cache '1'
        option concurrent '2'
        option idle_timeout '30'
        option minimal_ttl '0'
        option maximum_ttl '86400'
        option enable_pipeline '0'
        option insecure_skip_verify '0'
        option dns_leak '1'
        option cloudflare '0'
        option listen_port_api '9091'
        option bootstrap_dns '223.5.5.5'
        option redirect '1'
        option prefer_ipv4 '1'
        option enable_ecs_remote '0'
        option cache_size '2000'
        option lazy_cache_ttl '86400'
        option dump_file '0'
        list remote_dns 'https://8.8.8.8/dns-query'
        list remote_dns 'https://1.1.1.1/dns-query'
        option adblock '1'
        list ad_source 'https://raw.githubusercontent.com/Ranx5/AWAvenue-Ads-Rule/refs/heads/main/Filters/AWAvenue-Ads-Rule-Mosdns_v5.txt'
